<TabbedCompetition>
    do_default_tab: False
    canvas.before:
        Color:
            rgba: 0, 0, 0, 0
        Rectangle:
            size: self.size
            pos: self.pos

<CompetitionBracket>
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            size: self.size
            pos: self.pos
    Label:
        background_color: 1, 0, 1, 1

